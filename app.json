{
  "expo": {
    "name": "adustech",
    "slug": "adustech",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "adustech",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/plash.png",
      "resizeMode": "contain",
      "backgroundColor": "#E6F4FE"
    },
    "ios": {
      "infoPlist": {
        "NSCalendarsUsageDescription": "We use your calendar to add events you choose."
      },
      "supportsTablet": true,
      "splash": {
        "image": "./assets/images/plash.png",
        "resizeMode": "contain",
        "backgroundColor": "#E6F4FE",
        "dark": {
          "image": "./assets/images/plash.png",
          "backgroundColor": "#0A1929"
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "splash": {
        "image": "./assets/images/plash.png",
        "resizeMode": "contain",
        "backgroundColor": "#E6F4FE",
        "dark": {
          "image": "./assets/images/plash.png",
          "backgroundColor": "#0A1929"
        }
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "splash": {
        "image": "./assets/images/plash.png",
        "resizeMode": "contain",
        "backgroundColor": "#E6F4FE"
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/plash.png",
          "imageWidth": 300,
          "resizeMode": "contain",
          "backgroundColor": "#E6F4FE",
          "dark": {
            "image": "./assets/images/plash.png",
            "backgroundColor": "#0A1929"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
